struct task_list {
	uint_32 tid;
	uint_32 deadline;
	uint_32 task_type;
	uint_32 creation_time;
	struct task_list *next_cell;
	struct task_list *previous_cell;
};

struct overdue_tasks {
	uint_32 tid;
	uint_32 deadline;
	uint_32 task_type;
	uint_32 creation_time;
	struct overdue_tasks *next_cell;
	struct overdue_tasks *previous_cell;
};

