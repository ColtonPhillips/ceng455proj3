#ifndef SOURCES_PRIORITY_H_
#define SOURCES_PRIORITY_H_

#include "Cpu.h"
#include "mqx_ksdk.h"

_mqx_uint prioritygetmy() {
	_mqx_uint oldpriority;
	_mqx_uint newpriority = _task_get_priority(_task_get_id(),&oldpriority);
	printf("%d\n",newpriority);
	return PRIORITY_RTOS_TO_OSA(newpriority);
}

#endif /* SOURCES_PRIORITY_H_ */
